/* pages/loginPage/loginPage.wxss */
page{
  height: 100%;
}
.title{
  font-size: 40rpx;
}
.grayHolder{
  height: 100%;
  background: #ddd;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.line{
  border-top: 1px solid #aaa;
  transform: scaleY(0.2);  
}
.tips{
  padding: 36rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  width: 70%;

}
.info-list{
  display: flex;
  align-items: center;
  margin-top: 32rpx;
  color: #888;
}
.info-list>dot{
  margin-right: 16rpx; 
}
image{
  width: 100rpx;
  height: 100rpx
}
dot{
  display: block;
  border-radius: 100%;
  background: #aaa;
  width: 20rpx;
  height: 20rpx;
}
.message{
  display: flex;
  flex-direction: row;
  align-items: center;
}
button:after{
  border: none;
}
button{
  color: #888;
  font-size: 30rpx;
  width: 100rpx;
  padding-left:0;
  padding-right:0;
  background: none;
  margin-left: 0;
  margin-right: 0;
}
.active{
  color: #2BB675;
}
.options{
  margin-top: 50rpx;
  display: flex;
  justify-content: flex-end;
}
image{
  margin-right: 30rpx;
  background-size: 100rpx 100rpx;
  background-image: url("data:image/jpeg;base64,");
}