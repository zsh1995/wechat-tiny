page{
  display: block;
  box-sizing: padding-box;
  height:100%;
  overflow: visible;
}
.header{
  position: relative;
  padding: 27rpx 30rpx 30rpx 30rpx;
  font-size: 36rpx;
  line-height: 50rpx;
  background: #fff;
}
.container{
 display: flex;
 height: auto;
 width: auto;
 flex-direction: row;
 justify-content:center;
 align-items: flex-start;
 position : relative;
 flex-wrap: nowrap;
 background :#fff;
}
.number{
  position: absolute;
  left: 30rpx;
  top: 50%;
  width: 40rpx;
  height: 40rpx;
  margin: -20rpx 0 0;
  border: 1rpx solid #f6f6f6;
  border-radius: 20rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 28rpx;
  font-weight: lighter;
  color: #666;
}
.list{
  position: relative;
  padding: 0rpx 0rpx 0rpx 0rpx;
  margin-left: 2px;
  margin-right: 2px;
  width: 130rpx;
  height: 130rpx;
  font-size: 28rpx;
  color: #333;
}
.list_border{
  Border:1px solid #40E0D0;
  position: relative;
  padding: 0rpx 0rpx 0rpx 0rpx;
  width: 129rpx;
  height: 129rpx;
}
.list_sure{
  position: relative;
  padding: 0rpx 00rpx 0rpx 0rpx;
  width: 650rpx;
  height: 130rpx;
  font-size: 40rpx;
  text-align: center;
  color: #333;
}
.list_sure_border{
  position: relative;
  Border:1px solid #40E0D0;
  padding: 0rpx 00rpx 0rpx 0rpx;
  width: 649rpx;
  height: 129rpx;
  font-size: 40rpx;
  text-align: center;
  color: #333;
}
.xuanz {
  display:inline-block;
  width: 80rpx;
  height: 34rpx;
  margin: -5rpx 0 0;
  border-radius: 5rpx;
  background: #00bcd5;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
  vertical-align: middle;  
}
.tips {
  display:block;
  float:right;
  width: 80rpx;
  height: 34rpx;
  margin: 5rpx 0 0;
  border-radius: 5rpx;
  background: #ffbcd5;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
  vertical-align: middle;  
}

.mask{
  position: absolute;
}

page{
  height:100%;
}
.swiper-lists{
  position: relative;
  width: 100%;
  min-height:100%;
  overflow-x: hidden;
}

.swiper-lists .swiper-list{
  position: absolute;
  width: 100%;
  height:100%;
}

.swiper-lists .swiper-list.prev{
  top: 0;  
	transform: translate3d(-100%, 0 , 0px); 
  -webkit-transform: translate3d(-100%, 0 , 0px); 
}
.swiper-lists .swiper-list.next{
  top: 0;  
	transform: translate3d(100%, 0 , 0px); 
	-webkit-transform: translate3d(100%, 0 , 0px); 
}


.footer-layer{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
}
.footer{
  position: fixed;
  box-sizing: border-box;
  bottom: 0;
  width: 100%;
  transform: translate3d(0, 100%, 0px);
  -webkit-transform: translate3d(0, 100%, 0px);
  z-index: 100;
}
.count{
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: flex-start;
  box-sizing: border-box;
  top: -93rpx;
  width: 100%;
  padding: 30rpx 100rpx;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  zoom:1;
  z-index:1000;
}
.count view{
  float: left;
  line-height: 34rpx;
}
.count .collect{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .collect.active{
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;  
}
.count .remove{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  margin-left: 20rpx;
  background: url("data:image/png;base64,") no-repeat left center/38rpx 38rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .page{
  height:40rpx;
  padding: 0 0 0 47rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  line-height: 44rpx;
}
.count .error{ 
  height:40rpx;
  padding-left: 50rpx;
  background: url("data:image/png;base64,") no-repeat left center/40rpx 40rpx;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.count .success{ 
  height:40rpx;
  padding-left: 50rpx;
  background: url("data:image/png;base64,") no-repeat left center/40rpx 40rpx;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.clear { zoom:1; overflow:hidden!important; }