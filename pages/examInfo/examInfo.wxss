.mask{
  position: absolute;
}

page{
  height:100%;
}
.swiper-lists{
  position: relative;
  width: 100%;
  min-height:100%;
  overflow-x: hidden;
}

.swiper-lists .swiper-list{
  position: absolute;
  width: 100%;
  height:100%;
}

.swiper-lists .swiper-list.prev{
  top: 0;  
	transform: translate3d(-100%, 0 , 0px); 
  -webkit-transform: translate3d(-100%, 0 , 0px); 
}
.swiper-lists .swiper-list.next{
  top: 0;  
	transform: translate3d(100%, 0 , 0px); 
	-webkit-transform: translate3d(100%, 0 , 0px); 
}


.footer-layer{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
}
.footer{
  position: fixed;
  box-sizing: border-box;
  bottom: 0;
  width: 100%;
  transform: translate3d(0, 100%, 0px);
  -webkit-transform: translate3d(0, 100%, 0px);
  z-index: 100;
}
.count{
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
  align-items: flex-start;
  box-sizing: border-box;
  top: -93rpx;
  width: 100%;
  padding: 30rpx 50rpx;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  zoom:1;
  z-index:1000;
}
.count view{
  float: left;
  line-height: 34rpx;
}
.count .collect{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .collect.active{
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;  
}
.count .remove{
  float: left;
  height: 40rpx;
  padding-left: 45rpx;
  margin-left: 20rpx;
  background: url("data:image/png;base64,") no-repeat left center/38rpx 38rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 44rpx;
}
.count .page{
  height:40rpx;
  padding: 0 0 0 47rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  line-height: 44rpx;
}
.count .error{ 
  height:12rpx;
  width:100rpx;
  background: url("data:image/jpeg;base64,") no-repeat;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.count .success{ 
  height:12rpx;
  width:100rpx;
  background: url("data:image/jpeg;base64,") no-repeat;
  font-size: 24rpx;
  color:#999;
  vertical-align: middle;
  line-height: 44rpx;
  }
.clear { zoom:1; overflow:hidden!important; }

.maincontent{
  line-height:1.6;
  letter-spacing: 0.1em;
}
.tipEnd{
  margin-top: 36rpx;
  align-self: center;
  color: #aaa;
}